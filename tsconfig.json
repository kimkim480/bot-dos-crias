{
  "compilerOptions": {
    "rootDir": "./",
    "baseUrl": "./src",
    "outDir": "./dist",
    "target": "es2022",
    "module": "commonjs",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}